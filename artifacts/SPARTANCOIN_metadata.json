		"devdoc": {
			"methods": {
				"allowance(address,address)": {
					"details": "Function to check the amount of tokens that an owner allowed to a spender.",
					"params": {
						"owner": "address The address which owns the funds.",
						"spender": "address The address which will spend the funds."
					},
					"return": "A uint256 specifying the amount of tokens still available for the spender."
				},
				"approve(address,uint256)": {
					"details": "Approve the passed address to spend the specified amount of tokens on behalf of msg.sender. Beware that changing an allowance with this method brings the risk that someone may use both the old and the new allowance by unfortunate transaction ordering. One possible solution to mitigate this race condition is to first reduce the spender's allowance to 0 and set the desired value afterwards",
					"params": {
						"spender": "The address which will spend the funds.",
						"value": "The amount of tokens to be spent."
					}
				},
				"balanceOf(address)": {
					"details": "Gets the balance of the specified address.",
					"params": {
						"owner": "The address to query the balance of."
					},
					"return": "A uint256 representing the amount owned by the passed address."
				},
				"burn(uint256)": {
					"details": "function that burns an amount of the token of a given account.",
					"params": {
						"_amount": "The amount that will be burnt."
					}
				},
				"decimals()": {
					"return": "the number of decimals of the token."
				},
				"decreaseAllowance(address,uint256)": {
					"details": "Decrease the amount of tokens that an owner allowed to a spender. approve should be called when _allowed[msg.sender][spender] == 0. To decrement allowed value is better to use this function to avoid 2 calls (and wait until the first transaction is mined) Emits an Approval event.",
					"params": {
						"spender": "The address which will spend the funds.",
						"subtractedValue": "The amount of tokens to decrease the allowance by."
					}
				},
				"getAvailableBalance(address)": {
					"params": {
						"lockedAddress": "The address to check."
					},
					"return": "the total amount of locked funds of a given address."
				},
				"getBurnedAmountTotal()": {
					"return": "the total amount of burned coins \r"
				},
				"getCirculatingSupplyTotal()": {
					"return": "the total amount of circulating coins that are not locked at the current time \r"
				},
				"getLockedAddresses()": {
					"return": "the list of all addresses that have at least a fund locked currently or in the past"
				},
				"getLockedAddressesCurrently()": {
					"return": "the list of all addresses that have at least a fund locked currently"
				},
				"getLockedAmount(address)": {
					"params": {
						"lockedAddress": "The address to check."
					},
					"return": "the total amount of locked funds of a given address."
				},
				"getLockedAmountTotal()": {
					"return": "the total amount of locked funds at the current time"
				},
				"getNumberOfLockedAddresses()": {
					"return": "the number of addresses that have at least a fund locked currently or in the past"
				},
				"getNumberOfLockedAddressesCurrently()": {
					"return": "the number of addresses that have at least a fund locked currently"
				},
				"increaseAllowance(address,uint256)": {
					"details": "Increase the amount of tokens that an owner allowed to a spender. approve should be called when _allowed[msg.sender][spender] == 0. To increment allowed value is better to use this function to avoid 2 calls (and wait until the first transaction is mined) Emits an Approval event.",
					"params": {
						"addedValue": "The amount of tokens to increase the allowance by.",
						"spender": "The address which will spend the funds."
					}
				},
				"isOwner()": {
					"return": "true if `msg.sender` is the owner of the contract."
				},
				"name()": {
					"return": "the name of the token."
				},
				"owner()": {
					"return": "the address of the owner."
				},
				"renounceOwnership()": {
					"details": "Allows the current owner to relinquish control of the contract. It will not be possible to call the functions with the `onlyOwner` modifier anymore."
				},
				"symbol()": {
					"return": "the symbol of the token."
				},
				"totalSupply()": {
					"details": "Total number of tokens in existence"
				},
				"transfer(address,uint256)": {
					"details": "transfer of token to another address. always require the sender has enough balance",
					"params": {
						"_amount": "The amount to be transferred.",
						"_receiver": "The address to transfer to."
					},
					"return": "the bool true if success. "
				},
				"transferAndLock(address,uint256,uint256)": {
					"details": "transfer to a given address a given amount and lock this fund until a given time used for sending fund to team members, partners, or for owner to lock service fund over time",
					"params": {
						"_amount": "The amount to transfer.",
						"_receiver": "The address to transfer to.",
						"_releaseDate": "The date to release token."
					},
					"return": "the bool true if success."
				},
				"transferFrom(address,address,uint256)": {
					"details": "transfer of token on behalf of the owner to another address.  always require the owner has enough balance and the sender is allowed to transfer the given amount",
					"params": {
						"_amount": "The amount to be transferred.",
						"_from": "The address to transfer from.",
						"_receiver": "The address to transfer to."
					},
					"return": "the bool true if success. "
				},
				"transferOwnership(address)": {
					"details": "Allows the current owner to transfer control of the contract to a newOwner.",
					"params": {
						"newOwner": "The address to transfer ownership to."
					}
				}
			}
		},
